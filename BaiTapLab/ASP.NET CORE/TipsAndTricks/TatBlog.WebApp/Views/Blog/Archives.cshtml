@model IEnumerable<Post>

<h1>Archives</h1>
<ul>
    @foreach (var post in Model)
    {
        <li>
            <a href="@Url.Action("Post", "Blog", new { Year = post.PostedDate.Year, Month = post.PostedDate.Month, Day = post.PostedDate.Day, Slug = post.UrlSlug })">@post.Title</a>
        </li>
    }
</ul>